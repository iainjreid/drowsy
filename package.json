{
  "name": "drowsy",
  "version": "4.1.0",
  "description": "A lazy integration tool for RESTful interfaces",
  "main": "lib/drowsy.js",
  "types": "lib/drowsy.d.ts",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "prettier": "prettier --write *.{json,md} lib/*",
    "lint": "eslint lib --fix",
    "test": "jest lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iainreid820/drowsy.git"
  },
  "keywords": [
    "api",
    "apis",
    "rest",
    "restful",
    "integration"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iainreid820/drowsy/issues"
  },
  "homepage": "https://github.com/iainreid820/drowsy#readme",
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^23.6.0",
    "prettier": "^1.17.0"
  },
  "greenkeeper": {
    "commitMessages": {
      "initialBadge": "Add Greenkeeper badge",
      "initialDependencies": "Update dependencies",
      "initialBranches": "Whitelist greenkeeper branches",
      "dependencyUpdate": "Update ${dependency} to version ${version}",
      "devDependencyUpdate": "Update ${dependency} to version ${version}",
      "dependencyPin": "Pin ${dependency} to ${oldVersion}",
      "devDependencyPin": "Pin ${dependency} to ${oldVersion}"
    }
  },
  "jest": {
    "coverageDirectory": "./coverage",
    "collectCoverage": true
  },
  "prettier": {
    "printWidth": 120,
    "proseWrap": "always"
  }
}
