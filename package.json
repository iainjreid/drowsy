{
  "name": "drowsy",
  "version": "3.0.0",
  "description": "A lazy integration tool for RESTful interfaces",
  "main": "dist/Drowsy.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "prettier": "prettier --write \"{,!(node_modules)/}**/*.{js,json,md}\"",
    "lint": "eslint . --fix",
    "test": "echo \"Warning: no test specified\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iainreid820/drowsy.git"
  },
  "keywords": [
    "api",
    "apis",
    "rest",
    "restful",
    "integration"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iainreid820/drowsy/issues"
  },
  "homepage": "https://github.com/iainreid820/drowsy#readme",
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "prettier": "^1.14.3"
  },
  "greenkeeper": {
    "commitMessages": {
      "initialBadge": "Add Greenkeeper badge",
      "initialDependencies": "Update dependencies",
      "initialBranches": "Whitelist greenkeeper branches",
      "dependencyUpdate": "Update ${dependency} to version ${version}",
      "devDependencyUpdate": "Update ${dependency} to version ${version}",
      "dependencyPin": "Pin ${dependency} to ${oldVersion}",
      "devDependencyPin": "Pin ${dependency} to ${oldVersion}"
    }
  },
  "prettier": {
    "printWidth": 120,
    "proseWrap": "always"
  }
}
